<settings>

    <setting propertyKey="@Properties.VERSION_ID" title="@Strings.VERSION_TITLE">
        <settingConfig type="alphaNumeric" readonly="true" />
    </setting>

    <setting propertyKey="@Properties.DATA_SOURCE_ID" title="@Strings.DATA_SOURCE_TITLE">
        <settingConfig type="list" >
            <listEntry value="0">@Strings.week</listEntry>
            <listEntry value="1">@Strings.rolling7d</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.ACUTE_ID" title="@Strings.ACUTE_TITLE">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.CHRONIC_ID" title="@Strings.CHRONIC_TITLE">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.BONUS_1_ID" title="@Strings.BONUS_1_TITLE">
        <settingConfig type="list" >
            <listEntry value="0">@Strings.counter</listEntry>
            <listEntry value="1">@Strings.vo2</listEntry>
            <listEntry value="2">@Strings.avg_rhr</listEntry>
            <listEntry value="7">@Strings.rhr</listEntry>
            <listEntry value="3">@Strings.avg_hr</listEntry>
            <listEntry value="4">@Strings.resting_time</listEntry>
            <listEntry value="5">@Strings.steps</listEntry>
            <listEntry value="6">@Strings.free_one</listEntry>
            <listEntry value="8">@Strings.seconds</listEntry>
            <listEntry value="9">@Strings.altitude</listEntry>
            <listEntry value="10">@Strings.ac_ratio</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.BONUS_2_ID" title="@Strings.BONUS_2_TITLE">
        <settingConfig type="list" >
            <listEntry value="0">@Strings.counter</listEntry>
            <listEntry value="1">@Strings.vo2</listEntry>
            <listEntry value="2">@Strings.avg_rhr</listEntry>
            <listEntry value="7">@Strings.rhr</listEntry>
            <listEntry value="3">@Strings.avg_hr</listEntry>
            <listEntry value="4">@Strings.resting_time</listEntry>
            <listEntry value="5">@Strings.steps</listEntry>
            <listEntry value="6">@Strings.free_one</listEntry>
            <listEntry value="8">@Strings.seconds</listEntry>
            <listEntry value="9">@Strings.altitude</listEntry>
            <listEntry value="10">@Strings.ac_ratio</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.RACE_DATE_ID" title="@Strings.RACE_DATE_TITLE">
        <settingConfig type="date" />
    </setting>

    <setting propertyKey="@Properties.NAME_ID" title="@Strings.NAME_TITLE">
        <settingConfig type="alphaNumeric" />
    </setting>

    <setting propertyKey="@Properties.DATA_ID" title="@Strings.DATA_TITLE">
        <settingConfig type="alphaNumeric" />
    </setting>

    <setting propertyKey="@Properties.BackgroundColor" title="@Strings.BackgroundColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <!-- <listEntry value="0x555555">@Strings.ColorDarkGray</listEntry>
            <listEntry value="0xAAAAAA">@Strings.ColorLightGray</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry> -->
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.ForegroundColor" title="@Strings.ForegroundColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <listEntry value="0x0000FF">@Strings.ColorBlue</listEntry>
            <listEntry value="0xFF0000">@Strings.ColorRed</listEntry>
            <listEntry value="0x00FF00">@Strings.ColorGreen</listEntry>
            <listEntry value="0xFF8200">@Strings.ColorOrange</listEntry>
            <listEntry value="0xdb006c">@Strings.ColorPurple</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.Strip" title="@Strings.stripTitle">
        <settingConfig type="boolean" />
    </setting>

</settings>
